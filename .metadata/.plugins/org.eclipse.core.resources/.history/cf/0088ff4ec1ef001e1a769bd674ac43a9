package fundamentos;

public class ConversaoTemperatura {
	//método main é ponto de entrada no programa
public static void main(String[] args) {
	double temperaturaF = 13.5;
	double temperaturaC;
	final int constanteSubtracao = 32;
	final double constanteDivisao = 5/9.0;
	
	temperaturaC = (constanteSubtracao - 32)/constanteDivisao;
	
	System.out.println(temperaturaF + "°F equivale a " + temperaturaC + "°C");
}
}
