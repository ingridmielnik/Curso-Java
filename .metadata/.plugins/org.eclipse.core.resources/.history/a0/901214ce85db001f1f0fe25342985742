package classes;

public class Data {
	int dia;
	int mes;
	int ano;
	
	Data(){
		dia = 1;
		mes = 1;
		ano = 1970;
	}
	
	Data(int dia, int mes, int ano){
		
	}
	
	String obterDataFormata() {
		return String.format("%d/%d/%d", dia, mes, ano);
	} 
	
//	não é recomendado - system só imprime no terminal
	void imprimirDataFormatada() {
		System.out.printf("%d/%d/%d\n", dia, mes, ano);
	}
}