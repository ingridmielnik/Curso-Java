package classes;

public class DataTeste {
	public static void main(String[] args) {
		Data d1 = new Data();
		d1.dia = 7;
		d1.mes = 11;
		d1.ano = 2021;
		
		Data d2 = new Data();
		d2.dia = 12;
		d2.mes = 12;
		d2.ano = 2012;
		
		Data d3 = new Data();
		
		String dataFormatada1 = d1.obterDataFormata();
		System.out.println(dataFormatada1);
		d2.imprimirDataFormatada();
		d3.imprimirDataFormatada();
	}
}
